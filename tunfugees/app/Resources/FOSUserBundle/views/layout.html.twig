    <link href="{{ asset ('assets/css/style.css') }}" rel="stylesheet" type="text/css">
    <link href="{{ asset ('assets/css/dashboard.widgets.css') }}" rel="stylesheet" type="text/css" media="screen"/>


</head>

<body>
<div>
    {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
        {{ 'layout.logged_in_as'|trans({'%username%': app.user.username}, 'FOSUserBundle') }}({{ app.user.nom | upper }} {{ app.user.prenom }})|
        <a href="{{ path('fos_user_security_logout') }}"> DÃ©connexion
            {#{{ 'layout.logout'|trans({}, 'FOSUserBundle') }}#}
        </a>
    {% else %}
        {#<a href="{{ path('fos_user_security_login') }}">Se Connecter</a>#}
    {% endif %}
</div>

{% if app.request.hasPreviousSession %}
    {% for type, messages in app.session.flashbag.all() %}
        {% for message in messages %}
            <div class="flash-{{ type }}">
                {{ message }}
            </div>
        {% endfor %}
    {% endfor %}
{% endif %}

<div>
    {% block fos_user_content %}
    {% endblock fos_user_content %}
</div>
</body>
